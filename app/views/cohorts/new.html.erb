<h1>Create Your cohort</h1>
<%= form_for @cohort do |f| %>
<label>name</label><br>
  <%= f.text_field :name %><br>

  <label>start date</label><br>
  <%= f.date_field :start_date %><br>
  <label>start date</label><br>
  <%= f.date_field :end_date %><br>

  <label>student?</label><br>
  <%= collection_select(:cohort, :student_id, Student.all, :id, :first_name,{include_blank: true}) %><br>


<label>instructor?</label><br>
<%= collection_select(:cohort, :instructor_id, Instructor.all, :id, :first_name,  {include_blank: true}) %><br>


<p>
<%= f.submit %>
</p>
<% end %>
