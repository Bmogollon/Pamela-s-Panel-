<h1>Create Your instructors</h1>
<%= form_for @instructor do |f| %>
<label>first name</label><br>
  <%= f.text_field :first_name %><br>
  <label>last name</label><br>
  <%= f.text_field :last_name %><br>
  <label>what is your age</label><br>

  <%= f.number_field :age %><br>
  <label>what is your salary</label><br>

  <%= f.number_field :salary %><br>

  <label>What is your education</label>
  <%= f.select :education, ["Highschool Diploma", "Bachlor Degree", "Master's Degree", "Ph.D"] %> <br>




<p>
<%= f.submit %>
</p>
<% end %>
