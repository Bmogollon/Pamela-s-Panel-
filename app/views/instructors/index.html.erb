<h2>All Instructor</h2>
<table>
  <tr>
  <th>First Name</th>
  <th>Last Name</th>

  <th>Age</th>
    <th>Salary</th>
  <th>Education</th>
  </tr>
<% @instructor.each do |instruct| %>
    <tr>
      <td><%= instruct.first_name %></td>
      <td><%= instruct.last_name %></td>
      <td><%= instruct.age %></td>
        <td><%= instruct.salary %></td>
      <td><%= instruct.education %></td>
      <td><a href="/instructors/<%=instruct.id%>">View instructors</a></td>
      <td><a href="/instructors/<%=instruct.id%>/edit"> Edit instructors </a></td>

      </tr>
    <% end %>
</table>
